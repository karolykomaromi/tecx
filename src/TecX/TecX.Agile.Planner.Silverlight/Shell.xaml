<UserControl x:Class="TecX.Agile.Planner.Shell"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:cal="clr-namespace:Microsoft.Practices.Prism.Regions;assembly=Microsoft.Practices.Prism" 
    xmlns:View="clr-namespace:TecX.Agile.View;assembly=TecX.Agile.View" 
    mc:Ignorable="d"
    x:Name="MainWindow"
    Width="800" 
    Height="600">
    <Grid 
        cal:RegionManager.RegionName="Main">
        <TextBlock Text="Success"/>
        <Canvas>
            <View:StoryCard 
                    x:Name="storyCard" 
                    Width="300" 
                    Height="200"/>
        </Canvas>
    </Grid>
</UserControl>
